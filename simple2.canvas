  if (attrPointVelocities   == null)  {  report(\"warning: pointcloud has no attribute 'pointVelocities'\");    return;  }
  if (attrPointForces       == null)  {  report(\"warning: pointcloud has no attribute 'pointForces'\");        return;  }
  if (attrVertexColors      == null)  {  report(\"warning: pointcloud has no attribute 'vertexColors'\");       return;  }
  if (attrPointSizes        == null)  {  report(\"warning: pointcloud has no attribute 'pointSizes'\");         return;  }

  // correct and check input parameters.
  Scalar  flockingStrength        = Math_max(0.0, Math_min(1.0, in_flockingStrength));
  Scalar  viewConeDistance        = Math_max(0.0, in_viewConeDistance);
  Scalar  viewCone                = Math_degToRad(Math_max(0.0, Math_min(180.0, 0.5 * in_viewConeDegrees)));
  Scalar  viewConeFalloff         = Math_max(0.0, Math_min(0.5 * viewCone, Math_degToRad(0.5 * in_viewConeDegreesFalloff)));
  UInt32  maxNeighbors            = Math_min(256, Math_max(0, in_maxNeighbors));
  Scalar  ignoreSmallerNeighbors  = Math_max(0.0, Math_min(1.0, in_ignoreSmallerNeighbors));
  Scalar  distanceToNeighbors     = Math_max(0.0, in_distanceToNeighbors);
  Scalar  privacyDistance         = Math_max(0.000001, in_privacyDistance);
  Scalar  privacyStrength         = Math_max(0.0, in_privacyStrength);
  Scalar  velocityAlignement      = Math_max(0.0, Math_min(1.0, in_velocityAlignement));
  Scalar  cohesionStrength        = Math_max(0.0, Math_min(1.0, in_cohesionStrength));
  Boolean useBoundingSpheres      = (in_useBoundingSpheres && boundingSpheres.size() == pointcloud.pointCount());
  if (   flockingStrength <= 0
      || viewConeDistance <= 0
      || viewCone         <= 0
      || maxNeighbors     <= 0)
    return;
  
  // amount of points in point cloud.
  UInt32 numPoints = geometryAttributes.size();
  
  // init array of new point velocities.
  Vec3 newPointVelocities[];
  newPointVelocities.resize(numPoints);
  for (UInt32 i=0;i<numPoints;i++)
    newPointVelocities[i] = attrPointVelocities.values[i];

  // ________
  // | main |
  {
    // build a temporary Points object from
    // the array of boudning spheres.
    Points pointsBoundingSpheres = Points();
    if (useBoundingSpheres)
    {
      pointsBoundingSpheres.addPoints(numPoints);
      pointsBoundingSpheres.getOrCreateSizesAttribute();
      for (UInt32 i=0;i<numPoints;i++)
      {
        Float32 x = boundingSpheres[i].x;
        Float32 y = boundingSpheres[i].y;
        Float32 z = boundingSpheres[i].z;
        Float32 t = boundingSpheres[i].t;
        pointsBoundingSpheres.setPosition(i, Vec3(x, y, z));
        pointsBoundingSpheres.setSize(i, t);
      }
      pointsBoundingSpheres.incrementPositionsVersion();
      pointsBoundingSpheres.incrementSizesVersion();
    }
    
    // create the reference at the Points object
    // that will be used for the spatial query.
    Ref<Points> pointsRefForSpacialQuery = (pointsBoundingSpheres.pointCount() > 0 ? pointsBoundingSpheres : pointcloud);
  
    // prepare the spatial query.
    pointsRefForSpacialQuery.prepareForSpatialQueries(numPoints, null);
    Ref<SpatialQuery> query = pointsRefForSpacialQuery.beginSpatialQuery();

    // calculate flocking velocities and store them in newPointVelocities[].
    UInt32 nIndices  [];
    Vec3   nVectors  [];
    Scalar nDistances[];
    nIndices  .resize(maxNeighbors);
    nVectors  .resize(maxNeighbors);
    nDistances.resize(maxNeighbors);
    for (UInt32 i=0;i<numPoints;i++)
    {
      // get the indices of the N closest neighbors.
      UInt32 numIndices  = 0;
      UInt32 numElements = query.getElementsInBSphere(attrPositions.values[i], viewConeDistance);
      if (numElements <= maxNeighbors)
      {
        // the trivial case:
        // nIndices[] is large enough to contain all elements.
        for (UInt32 j=0;j<numElements;j++)
        {
          UInt32 index = query.getQueriedElement(j);
          if (index != i)
          {
            nIndices  [numIndices] = index;
            nVectors  [numIndices] = pointsRefForSpacialQuery.getPosition(index) - attrPositions.values[i];
            nDistances[numIndices] = nVectors[numIndices].length();
            numIndices++;
          }
        }
      }
      else
      {
        // the non-trivial case:
        // nIndices[] cannot hold all elements,
        // so we use only the N closest ones.
        UInt32 j = 0;
        for (;j<numElements;j++)
        {
          UInt32 index = query.getQueriedElement(j);
          if (index != i)
          {
            nIndices  [numIndices] = index;
            nVectors  [numIndices] = pointsRefForSpacialQuery.getPosition(index) - attrPositions.values[i];
            nDistances[numIndices] = nVectors[numIndices].length();
            numIndices++;
            j++;
            break;
          }
        }
        for (;j<numElements;j++)
        {
          UInt32 index = query.getQueriedElement(j);
          if (index != i)
          {
            Vec3   nVector   = pointsRefForSpacialQuery.getPosition(index) - attrPositions.values[i];
            Scalar nDistance = nVector.length();
            if (nDistance >= nDistances[numIndices - 1])
            {
              if (numIndices < maxNeighbors)
              {
                nIndices  [numIndices] = index;
                nVectors  [numIndices] = nVector;
                nDistances[numIndices] = nDistance;
                numIndices++;
              }
            }
            else if (nDistance <= nDistances[0])
            {
              for (UInt32 k=Math_min(numIndices, maxNeighbors-1);k>0;k--)
              {
                nIndices  [k] = nIndices  [k - 1];
                nVectors  [k] = nVectors  [k - 1];
                nDistances[k] = nDistances[k - 1];
              } 
              nIndices  [0] = index;
              nVectors  [0] = nVector;
              nDistances[0] = nDistance;
              if (numIndices < maxNeighbors)
                numIndices++;
            }
            else
            {
              for (UInt32 k=1;k<numIndices;k++)
              {
                if (nDistance <= nDistances[k])
                {
                  for (UInt32 l=Math_min(numIndices, maxNeighbors-1);l>k;l--)
                  {
                    nIndices  [l] = nIndices  [l - 1];
                    nVectors  [l] = nVectors  [l - 1];
                    nDistances[l] = nDistances[l - 1];
                  } 
                  nIndices  [k] = index;
                  nVectors  [k] = nVector;
                  nDistances[k] = nDistance;
                  if (numIndices < maxNeighbors)
                    numIndices++;
                  break;
                }
              } 
            }
          }
        }
      }
      if (numIndices == 0)
        continue;

      // calculate.
      Vec3   vSeparation   = Vec3(0.0);
      Scalar maxSeparation = 0;
      Vec3   vAlign        = attrPointVelocities.values[i];
      Scalar divAlign      = 1;
      Vec3   pCohesion     = attrPositions.values[i];
      Scalar divCohesion   = 1;
      for (UInt32 j=0;j<numIndices;j++)
      {
        Scalar scaleRatio = 1;
        if (attrPointSizes.values[i] > 0)
        {
          scaleRatio = attrPointSizes.values[nIndices[j]] / attrPointSizes.values[i];
          if (scaleRatio < 1)
          {
            if (ignoreSmallerNeighbors < 1)  scaleRatio = Math_max(0.0, scaleRatio - ignoreSmallerNeighbors) / (1.0 - ignoreSmallerNeighbors);
            else                             continue;
          }
          scaleRatio *= scaleRatio;
        }
          
        Scalar surfDist = Math_max(0.0, nDistances[j] - attrPointSizes.values[i] - attrPointSizes.values[nIndices[j]]);

        if (surfDist < distanceToNeighbors && nDistances[j] > 0)
        {
          Scalar fSep;
          if (surfDist > privacyDistance)
          {
            fSep  = (distanceToNeighbors - surfDist) / (distanceToNeighbors - privacyDistance);
            fSep *= fSep;
          }
          else
          {
            fSep  = surfDist / privacyDistance;
            fSep *= fSep;
            fSep  = fSep * 1 + (1 - fSep) * privacyStrength;
          }

          fSep *= Math_min(1.0, scaleRatio);
          maxSeparation  = Math_max(maxSeparation, Math_min(1.0, fSep));

          vSeparation -= (fSep / nDistances[j]) * nVectors[j];
        }
          
        if (maxSeparation < 1)
        {
          Scalar nAngle = nVectors[j].angleTo(attrPointOrientations.values[i].rotateVector(Vec3(0.0, 0.0, 1.0)));
          
          Scalar wA;
          if      (nAngle >= viewCone)                    wA = 0;
          else if (nAngle <= viewCone - viewConeFalloff)  wA = 1;
          else                                            wA = (viewCone - nAngle) / (viewCone - viewConeFalloff);
          if (wA > 0)
          {
            if (surfDist > distanceToNeighbors && surfDist < viewConeDistance)
            {
              Scalar wD = (surfDist - distanceToNeighbors) / (viewConeDistance - distanceToNeighbors);
              if (wD >= 0.5)  {  wD = 2 * (1 - wD);  wD *= wD;  }
              else               wD = sqrt(2 * wD);

              Scalar f = scaleRatio * wA * wA * wD * wD;
              vAlign      += f * attrPointVelocities.values[nIndices[j]];
              divAlign    += f;
              pCohesion   += f * attrPositions.values[nIndices[j]];
              divCohesion += f;
            }
          }
        }
      }

      Vec3 vCohesion = 0.0;
      if (divCohesion != 1)
      {
        pCohesion /= divCohesion;
        vCohesion = cohesionStrength * (pCohesion - attrPositions.values[i]);
      }

      vAlign /= divAlign;
      Scalar factor;
      factor  = (1 - maxSeparation);
      factor *= velocityAlignement;
      newPointVelocities[i] = (1 - factor) * attrPointVelocities.values[i] + factor * vAlign;
      
      factor = maxSeparation;
      if (factor > 0)
        newPointVelocities[i] += factor * vSeparation;

      factor = (1 - maxSeparation);
      if (factor > 0)
        newPointVelocities[i] += factor * vCohesion;

      factor = flockingStrength;
      newPointVelocities[i] = (1 - factor) * attrPointVelocities.values[i] + factor * newPointVelocities[i];
    }

    // clean up.
    pointsRefForSpacialQuery.endSpatialQuery(query);
  }
    
  // replace current point velocities with new ones.
  for (UInt32 i=0;i<numPoints;i++)
    attrPointVelocities.values[i] = newPointVelocities[i];
  attrPointVelocities.incrementVersion();
}

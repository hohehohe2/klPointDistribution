            "execPortType" : "IO",
            "typeSpec" : "Execute"
            },
          {
            "objectType" : "ExecPort",
            "name" : "outPoints",
            "nodePortType" : "In",
            "execPortType" : "Out",
            "typeSpec" : "$TYPE$"
            },
          {
            "objectType" : "ExecPort",
            "name" : "scaleX",
            "nodePortType" : "Out",
            "execPortType" : "In",
            "typeSpec" : "Float32"
            },
          {
            "objectType" : "ExecPort",
            "name" : "scaleY",
            "nodePortType" : "Out",
            "execPortType" : "In",
            "typeSpec" : "Float32"
            }

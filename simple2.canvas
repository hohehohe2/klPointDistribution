        "code" : "dfgEntry
{
  // check inputs.
  if (pointcloud == null)
  {
    report(\"warning: pointcloud == null\");
    return;
  }
  
  // nothing to do?
  if (  !pointcloud.pointCount()
      || simulationStep == 0)
    return;

  // get/check attributes.
  String attrName;
  GeometryAttributes geometryAttributes = pointcloud.getAttributes();
  attrName = \"positions\";        Ref<Vec3Attribute> attrPositions       = geometryAttributes.getAttribute(attrName, Vec3Attribute);
  attrName = \"pointVelocities\";  Ref<Vec3Attribute> attrPointVelocities = geometryAttributes.getAttribute(attrName, Vec3Attribute);
  attrName = \"pointForces\";      Ref<Vec3Attribute> attrPointForces     = geometryAttributes.getAttribute(attrName, Vec3Attribute);
  if (attrPositions       == null)  {  report(\"warning: pointcloud has no attribute 'positions'\");        return;  }
  if (attrPointVelocities == null)  {  report(\"warning: pointcloud has no attribute 'pointVelocities'\");  return;  }
  if (attrPointForces     == null)  {  report(\"warning: pointcloud has no attribute 'pointForces'\");      return;  }
  
  // simulate.
  if (threading)
    report(\"warning: multithreading not yet implemented, switching to singlethreaded\");
  Boolean changesInPositions       = false;
  Boolean changesInPointVelocities = false;
  Boolean changesInPointForces     = false;
  for (UInt32 i=0;i<pointcloud.pointCount();i++)
  {
    if (!attrPointForces.values[i].equal(Vec3(0.0, 0.0, 0.0)))
    {
      attrPointVelocities.values[i] += simulationStep * attrPointForces.values[i];
      changesInPointVelocities = true;

      attrPointForces.values[i].setNull();
      changesInPointForces = true;
    }
    if (!attrPointVelocities.values[i].equal(Vec3(0.0, 0.0, 0.0)))
    {
      attrPositions.values[i] += simulationStep * attrPointVelocities.values[i];
      changesInPositions = true;
    }
  }
  
  // increment attribute versions.
  if (changesInPositions)        attrPositions      .incrementVersion();
  if (changesInPointVelocities)  attrPointVelocities.incrementVersion();
  if (changesInPointForces)      attrPointForces    .incrementVersion();
}

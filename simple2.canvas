        "code" : "dfgEntry
{
  // check inputs.
  if (pointcloud == null)
  {
    report(\"warning: pointcloud == null\");
    xfos.resize(0);
    return;
  }
  
  // init output.
  xfos.resize(pointcloud.pointCount());
  // nothing to do?
  if (!xfos.size())
    return;

  // get/check attributes.
  String attrName;
  GeometryAttributes geometryAttributes = pointcloud.getAttributes();
  attrName = \"positions\";          Ref<Vec3Attribute>   attrPositions         = geometryAttributes.getAttribute(attrName, Vec3Attribute);
  attrName = \"pointOrientations\";  Ref<QuatAttribute>   attrPointOrientations = geometryAttributes.getAttribute(attrName, QuatAttribute);
  attrName = \"pointSizes\";         Ref<ScalarAttribute> attrPointSizes        = geometryAttributes.getAttribute(attrName, ScalarAttribute);
  if (attrPositions         == null)  {  report(\"warning: pointcloud has no attribute 'positions'\");          return;  }
  if (attrPointOrientations == null)  {  report(\"warning: pointcloud has no attribute 'pointOrientations'\");  return;  }
  if (attrPointSizes        == null)  {  report(\"warning: pointcloud has no attribute 'pointSizes'\");         return;  }

  // fill xfos[].
  for (UInt32 i=0;i<pointcloud.pointCount();i++)
  {
    report(i + \" \" + attrPositions.values[i]);
    xfos[i].tr  = attrPositions.values[i];
    xfos[i].ori = attrPointOrientations.values[i];
    xfos[i].sc  = Vec3(scale * attrPointSizes.values[i]);
  }
}

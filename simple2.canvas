  }
  
  // init outputs.
  count = 0;
  positions = null;
  
  // get/check attributes.
  String attrName;
  GeometryAttributes geometryAttributes = pointcloud.getAttributes();
  attrName = \"positions\";          Ref<Vec3Attribute>   attrPositions         = geometryAttributes.getAttribute(attrName, Vec3Attribute);
  attrName = \"pointOrientations\";  Ref<QuatAttribute>   attrPointOrientations = geometryAttributes.getAttribute(attrName, QuatAttribute);
